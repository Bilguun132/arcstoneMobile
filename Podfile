target 'Arcstone Mobile' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
pod 'SVProgressHUD'
pod 'SideMenu'
pod 'SwiftyJSON'
pod 'Alamofire', '~> 4.3'
pod 'Firebase/Core'
pod 'Firebase/Messaging'
pod 'Batch', '~> 1.7'
pod 'Firebase/Auth'
  # Pods for Arcstone Mobile

  target 'Arcstone MobileTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'KIF'
    pod 'Nimble'
  end

end
